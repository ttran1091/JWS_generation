import jwt
import datetime
import hashlib

# Define the payload
payload = {
    'username': 'jws-test',
    'entity': 'apex',
    'datetime': str(datetime.datetime.now())
}

# Define the secret key
secret_key = '1234567890'

# Encode the JWS
jws = jwt.encode(payload, secret_key, algorithm='HS512')

print("JWS:")
print(jws)