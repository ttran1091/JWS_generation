import json
import datetime
from jose import jws

# Define the payload
payload = {
    'username': 'jws-test',
    'entity': 'apex',
    'datetime': datetime.datetime.utcnow().isoformat()
}

# Define the shared secret (used as the private key)
shared_secret = '1234567890'

# Create the JWS
jws_token = jws.sign(payload, shared_secret, algorithm='HS512')

print(jws_token)
